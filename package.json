{
  "name": "@soundws/custom-cf-resource-require-key-for-methods",
  "version": "1.0.4-beta.2",
  "description": "A custom cloudformation resource",
  "author": "The First Coders Team (https://www.firstcoders.co.uk)",
  "email": "support@firstcoders.co.uk",
  "bugs": "https://github.com/sound-ws/custom-cf-resource-require-key-for-methods/issues",
  "homepage": "https://github.com/sound-ws/custom-cf-resource-require-key-for-methods",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sound-ws/custom-cf-resource-require-key-for-methods.git"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "rm -rf .aws-sam dist && webpack --mode production && cp -R .aws-sam/build dist && sam build",
    "sam:publish": "sam build && sam package --template-file .aws-sam/build/template.yaml --output-template-file .aws-sam/build/packaged.yaml --s3-bucket $AWS_SAM_PACKAGE_BUCKET_NAME --region ${AWS_REGION:=us-east-1} && sam publish --template .aws-sam/build/packaged.yaml --region ${AWS_REGION:=us-east-1} --semantic-version $npm_package_version"
  },
  "dependencies": {
    "@aws-sdk/client-api-gateway": "^3.418.0",
    "cfn-response-async": "^1.0.0"
  },
  "devDependencies": {
    "aws-crt": "^1.15.15",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.49.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "license-webpack-plugin": "^4.0.2",
    "prettier": "^3.0.3",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
