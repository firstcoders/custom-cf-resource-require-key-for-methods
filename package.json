{
  "name": "@soundws/custom-cf-resource-require-key-for-methods",
  "version": "1.0.6-beta.0",
  "description": "A custom cloudformation resource",
  "author": "The First Coders Team (https://www.firstcoders.co.uk)",
  "email": "support@firstcoders.co.uk",
  "bugs": "https://github.com/sound-ws/custom-cf-resource-require-key-for-methods/issues",
  "homepage": "https://github.com/sound-ws/custom-cf-resource-require-key-for-methods",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sound-ws/custom-cf-resource-require-key-for-methods.git"
  },
  "files": [
    "src",
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "lint": "sam validate --lint",
    "build": "sam build && npm run build:licenses",
    "build:licenses": "node build/3rd-party-licenses.mjs",
    "prepublishOnly": "npm run build",
    "prepack": "rm -rf dist/ && mkdir -p dist/aws-sam && cp -R .aws-sam/build dist/aws-sam",
    "sam:publish": "npm run build && sam package --template-file .aws-sam/build/template.yaml --output-template-file .aws-sam/build/packaged.yaml --s3-bucket $AWS_SAM_PACKAGE_BUCKET_NAME --region $AWS_REGION && sam publish --template .aws-sam/build/packaged.yaml --region $AWS_REGION --semantic-version $npm_package_version"
  },
  "dependencies": {
    "@aws-sdk/client-api-gateway": "^3.598.0",
    "cfn-response-async": "^1.0.0"
  },
  "devDependencies": {
    "aws-crt": "^1.15.15",
    "esbuild": "^0.21.5",
    "esbuild-plugin-license": "^1.2.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.3.2"
  }
}
